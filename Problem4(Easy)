//Functions in C
#include<stdio.h>
int MAX(int a,int b,int c,int d )
{
  if(a>b&&a>b&&a>c&&a>d){
   return a;
}
  else if(b>a&&b>c&&b>d){
  return b;
}
  else if(c>a&&c>b&&c>d){
  return c;
}
 else
{
  return d;
}
}

int main(){
    int m,n,o,p;
    scanf("%d %d %d %d",&m,&n,&o,&p);
    int maximum = MAX(m,n,o,p);
    printf("%d",maximum);
    return 0;
    
}
